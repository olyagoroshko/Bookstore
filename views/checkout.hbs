        <!-- Start Checkout Area -->
        <section class="wn__checkout__area section-padding--lg bg__white">
            <div class="container">
                <div class="row">

                </div>
                <div class="row">
                    <div class="col-lg-6 col-12">
                        <div class="customer_details">
                            <h3>Billing details</h3>
                            <form class="customar__field" action="/checkout" method="post">
                                <div class="margin_between">
                                    <div class="input_box space_between">
                                        <label>First name <span>*</span></label>
                                        <input type="text" name="FCustName" value="{{customers.FCustName}}" required>
                                    </div>
                                    <div class="input_box space_between">
                                        <label>last name <span>*</span></label>
                                        <input type="text" name="LCustName" value="{{customers.LCustName}}" required>
                                    </div>
                                </div>
                                <div class="input__box">
                                    <label>Address <span>*</span></label>
                                    <input type="text" name="Address" value="{{customers.Address}}" required>
                                </div>

                                <div class="margin_between">
                                    <div class="input_box space_between">
                                        <label>Phone <span>*</span></label>
                                        <input type="tel" name="Phone" value="{{customers.Phone}}"
                                            pattern="[8029][0-9]{10}" required>
                                    </div>

                                    <div class="input_box space_between">
                                        <label>Email address <span>*</span></label>
                                        <input type="email" name="Email"
                                            pattern="[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,4}$"
                                            value="{{customers.Email}}" required>
                                    </div>
                                </div>
                                <div class="input__box">
                                    <label>Credit card № <span>*</span></label>
                                    <input type="text" name="CardNum" value="{{customers.CardNum}}" required>
                                </div>
                            </form>
                            <div class="wn__order">

                            <ul
                                class="cart__btn__list d-flex flex-wrap flex-md-nowrap flex-lg-nowrap justify-content-between ">
                                <li><button class="send-order" type="submit" href="">Order</a></li>
                            </ul>

                        </div>
                        </div>



                    </div>
                    <div class="col-lg-6 col-12 md-mt-40 sm-mt-40">
                        <div class="wn__order__box">
                            <h3 class="onder__title">Your order</h3>
